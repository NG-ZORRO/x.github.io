,"rawCode":"import { Component, signal } from '@angular/core';\nimport { FormsModule } from '@angular/forms';\n\nimport { NzFlexModule } from 'ng-zorro-antd/flex';\nimport { NzSwitchModule } from 'ng-zorro-antd/switch';\n\nimport { NxBubbleModule } from 'ng-zorro-x/bubble';\n\n@Component({\n  selector: 'nx-demo-bubble-loading',\n  imports: [FormsModule, NxBubbleModule, NzFlexModule, NzSwitchModule],\n  template: `\n    <nz-flex nzGap=\"large\" nzVertical>\n      <nx-bubble nxContent=\"hello world!\" [nxLoading]=\"loading()\" [nxAvatar]=\"{ icon: 'user' }\"> </nx-bubble>\n      <nz-flex nzGap=\"large\">\n        Loading State:\n        <nz-switch [ngModel]=\"loading()\" (ngModelChange)=\"loading.set($event)\"></nz-switch>\n      </nz-flex>\n    </nz-flex>\n  `\n})\nexport class NxDemoBubbleLoadingComponent {\n  loading = signal(true);\n}\n"}
