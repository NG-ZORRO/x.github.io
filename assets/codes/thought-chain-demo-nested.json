;\n\n@Component({\n  selector: 'nx-demo-thought-chain-nested',\n  imports: [NxThoughtChainModule, NzCardModule, NzButtonModule, NzIconModule],\n  template: `\n    <nz-card style=\"width: 500px\">\n      <nx-thought-chain [nxItems]=\"nxItems\" [nxCollapsible]=\"nxCollapsible\" />\n    </nz-card>\n\n    <ng-template #content1>\n      <nx-thought-chain [nxItems]=\"nxItems1\" />\n    </ng-template>\n    <ng-template #content2>\n      <nx-thought-chain [nxItems]=\"nxItems2\" />\n    </ng-template>\n\n    <ng-template #extraMore>\n      <button nz-button nzSize=\"small\" nzType=\"text\">\n        <nz-icon nzType=\"more\" nzTheme=\"outline\" />\n      </button>\n    </ng-template>\n\n    <ng-template #footer>\n      <button nz-button [nzBlock]=\"true\">Thought Chain Item Footer</button>\n    </ng-template>\n  `\n})\nexport class NxDemoThoughtChainNestedComponent {\n  @ViewChild('content1', { static: true }) contentElement1!: TemplateRef<void>;\n  @ViewChild('content2', { static: true }) contentElement2!: TemplateRef<void>;\n  @ViewChild('extraMore', { static: true }) extraMoreElement!: TemplateRef<void>;\n  @ViewChild('footer', { static: true }) footerElement!: TemplateRef<void>;\n\n  readonly nxItems: NxThoughtChainItem[] = [\n    {\n      title: 'Thought Chain Item Title',\n      description: 'description',\n      content: this.contentElement1,\n      extra: this.extraMoreElement,\n      footer: this.footerElement\n    },\n    {\n      title: 'Thought Chain Item Title',\n      description: 'description',\n      content: this.contentElement2,\n      extra: this.extraMoreElement,\n      footer: this.footerElement\n    }\n  ];\n  nxItems1: NxThoughtChainItem[] = [\n    {\n      title: '1-1 Thought Chain Item Title',\n      description: 'description',\n      extra: this.extraMoreElement\n    },\n    {\n      title: '1-2Thought Chain Item Title',\n      description: 'description',\n      extra: this.extraMoreElement\n    }\n  ];\n  nxItems2: NxThoughtChainItem[] = [\n    {\n      title: '2-1 Thought Chain Item Title',\n      description: 'description',\n      extra: this.extraMoreElement\n    }\n  ];\n\n  nxCollapsible = {\n    expandedKeys: [],\n    onExpand: (expandedKeys: string[]) => {\n      console.log(expandedKeys);\n    }\n  };\n}\n"}
